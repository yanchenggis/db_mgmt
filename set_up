# Check if the software is installed
Open Command Prompt (Win + R, type cmd) and type:
python --version
conda --version

where python
where conda


# Set up the libraries
Download VS Code & Python

conda create --name geo_env python=3.10 -y
conda activate geo_env
conda install -c conda-forge \
pandas geopandas numpy matplotlib seaborn \
contextily folium requests jupyter notebook \
rasterio fiona pyproj shapely gdal -y
